{"ast":null,"code":"import axios from 'axios'; // Import Axios\nimport Cookies from 'js-cookie'; // Import Cookies\n\nexport default {\n  props: {\n    showModal: Boolean,\n    task: Object // The task to be edited\n  },\n\n  data() {\n    return {\n      editedTask: {\n        // Initialize editedTask with the task's current values\n        title: this.task.title,\n        description: this.task.description,\n        date: this.task.date,\n        status: this.task.status,\n        id: this.task.id // Include the task ID for identification\n      }\n    };\n  },\n\n  methods: {\n    closeModal() {\n      this.$emit('close-edit-modal');\n    },\n    editTask() {\n      // Send a PUT request to update the task with editedTask data\n      axios.put(`http://localhost:8080/api/v1/tasks/${this.editedTask.id}`, this.editedTask) // Update the URL to your API endpoint\n      .then(response => {\n        // Task update was successful\n        console.log('Tarea editada:', response.data);\n\n        // Emit an event to notify the parent component to update the task\n        this.$emit('update-task', response.data);\n\n        // Close the modal\n        this.closeModal();\n      }).catch(error => {\n        // Task update failed\n        console.error('Error al editar la tarea:', error);\n\n        // Handle the error (e.g., show an error message to the user)\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","Cookies","props","showModal","Boolean","task","Object","data","editedTask","title","description","date","status","id","methods","closeModal","$emit","editTask","put","then","response","console","log","catch","error"],"sources":["D:\\Documents\\trabajos_semestre_7\\ing_software\\proyecto-1-TodoList\\TodoList-frontend\\src\\components\\EditTaskModal.vue"],"sourcesContent":["<template>\r\n  <div class=\"modal\" v-if=\"showModal\">\r\n    <!-- Modal content goes here -->\r\n    <div class=\"modal-content\">\r\n      <h2>Editar Tarea</h2>\r\n      <form @submit.prevent=\"editTask\">\r\n        <!-- Task edit form fields -->\r\n        <label for=\"title\">Título:</label>\r\n        <input type=\"text\" id=\"title\" v-model=\"editedTask.title\" required>\r\n\r\n        <label for=\"description\">Descripción:</label>\r\n        <input type=\"text\" id=\"description\" v-model=\"editedTask.description\" required>\r\n\r\n        <label for=\"date\">Fecha:</label>\r\n        <input type=\"date\" id=\"date\" v-model=\"editedTask.date\" required>\r\n\r\n        <label for=\"status\">Estado:</label>\r\n        <select id=\"status\" v-model=\"editedTask.status\" required>\r\n          <option value=\"Completada\">Completada</option>\r\n          <option value=\"Incompleta\">Incompleta</option>\r\n        </select>\r\n        <br>\r\n\r\n        <button class=\"buttons\" type=\"submit\">Guardar Cambios</button>\r\n        <button class=\"buttons\" type=\"button\" @click=\"closeModal\">Cancelar</button>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'; // Import Axios\r\nimport Cookies from 'js-cookie'; // Import Cookies\r\n\r\nexport default {\r\n  props: {\r\n    showModal: Boolean,\r\n    task: Object, // The task to be edited\r\n  },\r\n  data() {\r\n    return {\r\n      editedTask: {\r\n        // Initialize editedTask with the task's current values\r\n        title: this.task.title,\r\n        description: this.task.description,\r\n        date: this.task.date,\r\n        status: this.task.status,\r\n        id: this.task.id, // Include the task ID for identification\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    closeModal() {\r\n      this.$emit('close-edit-modal');\r\n    },\r\n    editTask() {\r\n      // Send a PUT request to update the task with editedTask data\r\n      axios\r\n        .put(`http://localhost:8080/api/v1/tasks/${this.editedTask.id}`, this.editedTask) // Update the URL to your API endpoint\r\n        .then((response) => {\r\n          // Task update was successful\r\n          console.log('Tarea editada:', response.data);\r\n\r\n          // Emit an event to notify the parent component to update the task\r\n          this.$emit('update-task', response.data);\r\n\r\n          // Close the modal\r\n          this.closeModal();\r\n        })\r\n        .catch((error) => {\r\n          // Task update failed\r\n          console.error('Error al editar la tarea:', error);\r\n\r\n          // Handle the error (e.g., show an error message to the user)\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Add CSS styles for your modal here */\r\n.modal {\r\n  /* Modal styles */\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  z-index: 999;\r\n}\r\n\r\n.buttons {\r\n  /* Button styles */\r\n  padding-top: 1vh;\r\n  padding-bottom: 1vh;\r\n  background-color: #63636395;\r\n  border-radius: 3vh;\r\n  justify-content: center;\r\n  text-align: center;\r\n  align-items: center;\r\n  padding: 10px;\r\n  padding-right: 50px;\r\n  margin: 20px;\r\n  color: #e4e2e2;\r\n  border-color: #68686800;\r\n  font-size: 12px;\r\n  transition: background-color 0.3s;\r\n  cursor: pointer;\r\n}\r\n\r\n.modal-content {\r\n  /* Modal content styles */\r\n  background-color: white;\r\n  padding: 20px;\r\n  border-radius: 5px;\r\n}\r\n</style>\r\n"],"mappings":"AA+BA,OAAOA,KAAI,MAAO,OAAO,EAAE;AAC3B,OAAOC,OAAM,MAAO,WAAW,EAAE;;AAEjC,eAAe;EACbC,KAAK,EAAE;IACLC,SAAS,EAAEC,OAAO;IAClBC,IAAI,EAAEC,MAAM,CAAE;EAChB,CAAC;;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE;QACV;QACAC,KAAK,EAAE,IAAI,CAACJ,IAAI,CAACI,KAAK;QACtBC,WAAW,EAAE,IAAI,CAACL,IAAI,CAACK,WAAW;QAClCC,IAAI,EAAE,IAAI,CAACN,IAAI,CAACM,IAAI;QACpBC,MAAM,EAAE,IAAI,CAACP,IAAI,CAACO,MAAM;QACxBC,EAAE,EAAE,IAAI,CAACR,IAAI,CAACQ,EAAE,CAAE;MACpB;IACF,CAAC;EACH,CAAC;;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,C