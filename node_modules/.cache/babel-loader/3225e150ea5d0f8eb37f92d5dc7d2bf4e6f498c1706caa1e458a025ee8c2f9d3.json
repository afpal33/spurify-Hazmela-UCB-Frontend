{"ast":null,"code":"import Cookies from 'js-cookie';\nimport axios from 'axios';\nimport EditTagModel from './EditTagModal.vue'; // Import your EditTagModel component\nimport CreateTagModal from './CreateTagModal.vue'; // Import your CreateTagModal component\n\nexport default {\n  name: 'AppEtiquetas',\n  data() {\n    return {\n      tags: [],\n      showEditModal: false,\n      tagToEdit: null,\n      showCreateTagModal: false\n    };\n  },\n  computed: {\n    hasUserIdCookie() {\n      return !!Cookies.get('userId');\n    },\n    filteredTags() {\n      return this.tags.filter(tag => tag.user_id === Cookies.get('userId'));\n    }\n  },\n  methods: {\n    goToPage() {\n      window.scrollTo({\n        top: 0,\n        behavior: 'smooth'\n      });\n    },\n    animateButton() {\n      // Add your button animation logic here\n    },\n    resetButton() {\n      // Add your button reset logic here\n    },\n    fetchTags() {\n      axios.get('http://localhost:8080/api/v1/tags/all').then(response => {\n        this.tags = response.data;\n      }).catch(error => {\n        console.error('Error fetching tags:', error);\n      });\n    },\n    confirmDeleteTag(tag) {\n      axios.delete(`http://localhost:8080/api/v1/tags/${tag.id}`).then(() => {\n        // Remove the deleted tag from the local list\n        const index = this.tags.indexOf(tag);\n        if (index !== -1) {\n          this.tags.splice(index, 1);\n        }\n      }).catch(error => {\n        console.error('Error deleting tag:', error);\n      });\n    },\n    editTag(tag) {\n      this.tagToEdit = tag;\n      this.showEditModal = true;\n    },\n    closeEditModal() {\n      this.showEditModal = false;\n      this.tagToEdit = null;\n    },\n    updateTag(updatedTag) {\n      // Implement your logic for updating the tag here\n      console.log('Updated tag:', updatedTag);\n    },\n    shoCreateTagModal() {\n      this.showCreateTagModal = true;\n    },\n    closeCreateTagModal() {\n      this.showCreateTagModal = false;\n    },\n    createTag(newTag) {\n      // Implement your logic for creating a new tag here\n      console.log('Created tag:', newTag);\n    }\n  },\n  mounted() {\n    if (this.hasUserIdCookie) {\n      this.fetchTags();\n    }\n  },\n  components: {\n    EditTagModel,\n    CreateTagModal\n  }\n};","map":{"version":3,"names":["Cookies","axios","EditTagModel","CreateTagModal","name","data","tags","showEditModal","tagToEdit","showCreateTagModal","computed","hasUserIdCookie","get","filteredTags","filter","tag","user_id","methods","goToPage","window","scrollTo","top","behavior","animateButton","resetButton","fetchTags","then","response","catch","error","console","confirmDeleteTag","delete","id","index","indexOf","splice","editTag","closeEditModal","updateTag","updatedTag","log","shoCreateTagModal","closeCreateTagModal","createTag","newTag","mounted","components"],"sources":["D:\\Documents\\trabajos_semestre_7\\ing_software\\proyecto-1-TodoList\\TodoList-frontend\\src\\components\\AppEtiquetas.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\" v-if=\"hasUserIdCookie\">\r\n    <div class=\"box\">\r\n      <h1>Lista de Etiquetas</h1>\r\n      <div class=\"tag-list\">\r\n        <table class=\"table-with-border\">\r\n          <thead>\r\n            <tr>\r\n              <th class=\"column-border\">Tag Title</th>\r\n              <th class=\"column-border\">Acciones</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"tag in filteredTags\" :key=\"tag.id\">\r\n              <td class=\"column-border\">{{ tag.tag }}</td>\r\n              <td class=\"column-border\">\r\n                <button @click=\"editTag(tag)\">Editar</button>\r\n                <button @click=\"confirmDeleteTag(tag)\">Eliminar</button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <h2 style=\"padding-top: 0vh; padding-bottom: 1vh;\"></h2>\r\n      <button @mouseover=\"animateButton\" @mouseleave=\"resetButton\" class=\"buttons\" @click=\"showCreateTagModal\">+</button>\r\n    </div>\r\n\r\n    <!-- Edit Tag Modal -->\r\n    <EditTagModel :showModal=\"showEditModal\" :tag=\"tagToEdit\" @close-edit-modal=\"closeEditModal\" @update-tag=\"updateTag\" />\r\n\r\n    <!-- Create Tag Modal -->\r\n    <CreateTagModal :showModal=\"shoCreateTagModal\" @close-create-modal=\"closeCreateTagModal\" @create-tag=\"createTag\" />\r\n  </div>\r\n  <div style=\"padding-bottom:15%;\" v-else>\r\n    <div class=\"box\">\r\n      <p>Debes iniciar sesi√≥n para acceder a tu lista de etiquetas.</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Cookies from 'js-cookie';\r\nimport axios from 'axios';\r\nimport EditTagModel from './EditTagModal.vue'; // Import your EditTagModel component\r\nimport CreateTagModal from './CreateTagModal.vue'; // Import your CreateTagModal component\r\n\r\nexport default {\r\n  name: 'AppEtiquetas',\r\n  data() {\r\n    return {\r\n      tags: [],\r\n      showEditModal: false,\r\n      tagToEdit: null,\r\n      showCreateTagModal: false,\r\n    };\r\n  },\r\n  computed: {\r\n    hasUserIdCookie() {\r\n      return !!Cookies.get('userId');\r\n    },\r\n    filteredTags() {\r\n      return this.tags.filter(tag => tag.user_id === Cookies.get('userId'));\r\n    },\r\n  },\r\n  methods: {\r\n    goToPage() {\r\n      window.scrollTo({ top: 0, behavior: 'smooth' });\r\n    },\r\n    animateButton() {\r\n      // Add your button animation logic here\r\n    },\r\n    resetButton() {\r\n      // Add your button reset logic here\r\n    },\r\n    fetchTags() {\r\n      axios.get('http://localhost:8080/api/v1/tags/all')\r\n        .then(response => {\r\n          this.tags = response.data;\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching tags:', error);\r\n        });\r\n    },\r\n    confirmDeleteTag(tag) {\r\n      axios\r\n        .delete(`http://localhost:8080/api/v1/tags/${tag.id}`)\r\n        .then(() => {\r\n          // Remove the deleted tag from the local list\r\n          const index = this.tags.indexOf(tag);\r\n          if (index !== -1) {\r\n            this.tags.splice(index, 1);\r\n          }\r\n        })\r\n        .catch(error => {\r\n          console.error('Error deleting tag:', error);\r\n        });\r\n    },\r\n    editTag(tag) {\r\n      this.tagToEdit = tag;\r\n      this.showEditModal = true;\r\n    },\r\n    closeEditModal() {\r\n      this.showEditModal = false;\r\n      this.tagToEdit = null;\r\n    },\r\n    updateTag(updatedTag) {\r\n      // Implement your logic for updating the tag here\r\n      console.log('Updated tag:', updatedTag);\r\n    },\r\n    shoCreateTagModal() {\r\n      this.showCreateTagModal = true;\r\n    },\r\n    closeCreateTagModal() {\r\n      this.showCreateTagModal = false;\r\n    },\r\n    createTag(newTag) {\r\n      // Implement your logic for creating a new tag here\r\n      console.log('Created tag:', newTag);\r\n    },\r\n  },\r\n  mounted() {\r\n    if (this.hasUserIdCookie) {\r\n      this.fetchTags();\r\n    }\r\n  },\r\n  components: {\r\n    EditTagModel,\r\n    CreateTagModal,\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style>\r\n.box {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  flex-direction: column;\r\n  backdrop-filter: blur(30px);\r\n  background-color: rgba(248, 248, 248, 0.135);\r\n  border-radius: 5vh;\r\n  width: 95%;\r\n  padding-top: 1vh;\r\n  padding-bottom: 3vh;\r\n}\r\n\r\n.container {\r\n  height: 59vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  padding-bottom: 12vh;\r\n}\r\n\r\n.tag-list {\r\n  color: white;\r\n}\r\n\r\n.buttons:hover {\r\n  background-color: #393939;\r\n}\r\n\r\n.buttons.jump {\r\n  animation: jumpAnimation 0.3s ease-in-out;\r\n}\r\n\r\n@keyframes jumpAnimation {\r\n  0% {\r\n    transform: translateY(0);\r\n  }\r\n  50% {\r\n    transform: translateY(-5px);\r\n  }\r\n  100% {\r\n    transform: translateY(0);\r\n  }\r\n}\r\n\r\n.buttons {\r\n  padding-top: 1vh;\r\n  padding-bottom: 1vh;\r\n  background-color: #63636395;\r\n  border-radius: 3vh;\r\n  width: 3%;\r\n  justify-content: center;\r\n  align-items: center;\r\n  color: #e4e2e2;\r\n  border-color: #68686800;\r\n  font-size: 25px;\r\n  transition: background-color 0.3s;\r\n  cursor: pointer;\r\n}\r\n\r\nh1 {\r\n  color: #e4e2e2;\r\n  font-size: 6vh;\r\n}\r\n\r\np {\r\n  color: #e4e2e2;\r\n  font-size: 4vh;\r\n}\r\n</style>\r\n"],"mappings":"AAyCA,OAAOA,OAAM,MAAO,WAAW;AAC/B,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,YAAW,MAAO,oBAAoB,EAAE;AAC/C,OAAOC,cAAa,MAAO,sBAAsB,EAAE;;AAEnD,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,aAAa,EAAE,KAAK;MACpBC,SAAS,EAAE,IAAI;MACfC,kBAAkB,EAAE;IACtB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,OAAO,CAAC,CAACX,OAAO,CAACY,GAAG,CAAC,QAAQ,CAAC;IAChC,CAAC;IACDC,YAAYA,CAAA,EAAG;MACb,OAAO,IAAI,CAACP,IAAI,CAACQ,MAAM,CAACC,GAAE,IAAKA,GAAG,CAACC,OAAM,KAAMhB,OAAO,CAACY,GAAG,CAAC,QAAQ,CAAC,CAAC;IACvE;EACF,CAAC;EACDK,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MACTC,MAAM,CAACC,QAAQ,CAAC;QAAEC,GAAG,EAAE,CAAC;QAAEC,QAAQ,EAAE;MAAS,CAAC,CAAC;IACjD,CAAC;IACDC,aAAaA,CAAA,EAAG;MACd;IAAA,CACD;IACDC,WAAWA,CAAA,EAAG;MACZ;IAAA,CACD;IACDC,SAASA,CAAA,EAAG;MACVxB,KAAK,CAACW,GAAG,CAAC,uCAAuC,EAC9Cc,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACrB,IAAG,GAAIqB,QAAQ,CAACtB,IAAI;MAC3B,CAAC,EACAuB,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;MAC9C,CAAC,CAAC;IACN,CAAC;IACDE,gBAAgBA,CAAChB,GAAG,EAAE;MACpBd,KAAI,CACD+B,MAAM,CAAE,qCAAoCjB,GAAG,CAACkB,EAAG,EAAC,EACpDP,IAAI,CAAC,MAAM;QACV;QACA,MAAMQ,KAAI,GAAI,IAAI,CAAC5B,IAAI,CAAC6B,OAAO,CAACpB,GAAG,CAAC;QACpC,IAAImB,KAAI,KAAM,CAAC,CAAC,EAAE;UAChB,IAAI,CAAC5B,IAAI,CAAC8B,MAAM,CAACF,KAAK,EAAE,CAAC,CAAC;QAC5B;MACF,CAAC,EACAN,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,qBAAqB,EAAEA,KAAK,CAAC;MAC7C,CAAC,CAAC;IACN,CAAC;IACDQ,OAAOA,CAACtB,GAAG,EAAE;MACX,IAAI,