{"ast":null,"code":"import axios from 'axios';\nimport Cookies from 'js-cookie';\nexport default {\n  props: {\n    showModal: Boolean,\n    tasks: Array // List of tasks to populate the combo box\n  },\n\n  data() {\n    return {\n      tag: {\n        title: '',\n        task_id: '',\n        // Task ID selected from the combo box\n        user_id: '' // User ID from the cookie\n      }\n    };\n  },\n\n  computed: {\n    hasUserIdCookie() {\n      return !!Cookies.get('userId');\n    }\n  },\n  methods: {\n    closeModal() {\n      this.$emit('close-create-modal');\n    },\n    createTag() {\n      // Ensure there is a user ID in the cookie\n      if (!this.hasUserIdCookie) {\n        console.error('User ID not found in the cookie.');\n        return;\n      }\n\n      // Set the user ID from the cookie\n      this.tag.user_id = Cookies.get('userId');\n\n      // Make the API request to create a new tag\n      axios.post('http://localhost:8080/api/v1/tags', this.tag).then(response => {\n        // Emit the newly created tag data to the parent component\n        this.$emit('create-tag', response.data);\n\n        // Clear the tag object for the next use\n        this.tag = {\n          title: '',\n          task_id: '',\n          user_id: ''\n        };\n\n        // Close the modal\n        this.closeModal();\n      }).catch(error => {\n        console.error('Error creating tag:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","Cookies","props","showModal","Boolean","tasks","Array","data","tag","title","task_id","user_id","computed","hasUserIdCookie","get","methods","closeModal","$emit","createTag","console","error","post","then","response","catch"],"sources":["D:\\Documents\\trabajos_semestre_7\\ing_software\\proyecto-1-TodoList\\TodoList-frontend\\src\\components\\CreateTagModal.vue"],"sourcesContent":["<template>\r\n    <div class=\"modal\" v-if=\"showModal\">\r\n      <div class=\"modal-content\">\r\n        <h2>Crear Nueva Etiqueta</h2>\r\n        <form @submit.prevent=\"createTag\">\r\n          <!-- Tag creation form fields -->\r\n          <label for=\"title\">TÃ­tulo:</label>\r\n          <input type=\"text\" id=\"title\" v-model=\"tag.title\" required>\r\n  \r\n          <!-- Combo box for selecting a task -->\r\n          <label for=\"task\">Seleccionar Tarea:</label>\r\n          <select id=\"task\" v-model=\"tag.task_id\" required>\r\n            <option value=\"\" disabled>Selecciona una tarea</option>\r\n            <option v-for=\"task in tasks\" :key=\"task.id\" :value=\"task.id\">{{ task.title }}</option>\r\n          </select>\r\n  \r\n          <br>\r\n  \r\n          <button class=\"buttons\" type=\"submit\">Guardar Etiqueta</button>\r\n          <button class=\"buttons\" type=\"button\" @click=\"closeModal\">Cancelar</button>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  import Cookies from 'js-cookie';\r\n\r\n  \r\n  export default {\r\n    props: {\r\n      showModal: Boolean,\r\n      tasks: Array, // List of tasks to populate the combo box\r\n    },\r\n    data() {\r\n      return {\r\n        tag: {\r\n          title: '',\r\n          task_id: '', // Task ID selected from the combo box\r\n          user_id: '', // User ID from the cookie\r\n        },\r\n      };\r\n    },\r\n    computed: {\r\n      hasUserIdCookie() {\r\n        return !!Cookies.get('userId');\r\n      },\r\n    },\r\n    methods: {\r\n      closeModal() {\r\n        this.$emit('close-create-modal');\r\n      },\r\n      createTag() {\r\n        // Ensure there is a user ID in the cookie\r\n        if (!this.hasUserIdCookie) {\r\n          console.error('User ID not found in the cookie.');\r\n          return;\r\n        }\r\n  \r\n        // Set the user ID from the cookie\r\n        this.tag.user_id = Cookies.get('userId');\r\n  \r\n        // Make the API request to create a new tag\r\n        axios\r\n          .post('http://localhost:8080/api/v1/tags', this.tag)\r\n          .then((response) => {\r\n            // Emit the newly created tag data to the parent component\r\n            this.$emit('create-tag', response.data);\r\n  \r\n            // Clear the tag object for the next use\r\n            this.tag = {\r\n              title: '',\r\n              task_id: '',\r\n              user_id: '',\r\n            };\r\n  \r\n            // Close the modal\r\n            this.closeModal();\r\n          })\r\n          .catch((error) => {\r\n            console.error('Error creating tag:', error);\r\n          });\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  \r\n<style scoped>\r\n/* Add CSS styles for your modal here */\r\n.modal {\r\n  /* Modal styles */\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  z-index: 999;\r\n}\r\n\r\n.buttons {\r\n  /* Button styles */\r\n  padding-top: 1vh;\r\n  padding-bottom: 1vh;\r\n  background-color: #63636395;\r\n  border-radius: 3vh;\r\n  justify-content: center;\r\n  text-align: center;\r\n  align-items: center;\r\n  padding: 10px;\r\n  padding-right: 50px;\r\n  margin: 20px;\r\n  color: #e4e2e2;\r\n  border-color: #68686800;\r\n  font-size: 12px;\r\n  transition: background-color 0.3s;\r\n  cursor: pointer;\r\n}\r\n\r\n.modal-content {\r\n  /* Modal content styles */\r\n  background-color: white;\r\n  padding: 20px;\r\n  border-radius: 5px;\r\n}\r\n</style>\r\n"],"mappings":"AA0BE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,WAAW;AAG/B,eAAe;EACbC,KAAK,EAAE;IACLC,SAAS,EAAEC,OAAO;IAClBC,KAAK,EAAEC,KAAK,CAAE;EAChB,CAAC;;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE;QACHC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE,EAAE;QAAE;QACbC,OAAO,EAAE,EAAE,CAAE;MACf;IACF,CAAC;EACH,CAAC;;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,OAAO,CAAC,CAACZ,OAAO,CAACa,GAAG,CAAC,QAAQ,CAAC;IAChC;EACF,CAAC;EACDC,OAAO,EA