{"ast":null,"code":"import Cookies from 'js-cookie';\nimport axios from 'axios';\nexport default {\n  name: 'TareasCompletadas',\n  props: {\n    filteredTasks: Array // List of filtered tasks from AppTareas\n  },\n\n  data() {\n    return {\n      completedTasks: [] // To store the fetched completed tasks\n    };\n  },\n\n  computed: {\n    hasUserIdCookie() {\n      return !!Cookies.get('userId'); // Check if the cookie 'userId' is present\n    },\n\n    // Filter completed tasks based on matching task IDs with filtered tasks\n    filteredCompletedTasks() {\n      const taskIds = this.filteredTasks.map(task => task.id);\n      return this.completedTasks.filter(completedTask => taskIds.includes(completedTask.task_id));\n    }\n  },\n  mounted() {\n    // Fetch completed tasks from the 'completed-tasks' API endpoint\n    axios.get('http://localhost:8080/api/v1/completed-tasks/all').then(response => {\n      this.completedTasks = response.data;\n    }).catch(error => {\n      console.error('Error fetching completed tasks:', error);\n    });\n  }\n};","map":{"version":3,"names":["Cookies","axios","name","props","filteredTasks","Array","data","completedTasks","computed","hasUserIdCookie","get","filteredCompletedTasks","taskIds","map","task","id","filter","completedTask","includes","task_id","mounted","then","response","catch","error","console"],"sources":["D:\\Documents\\trabajos_semestre_7\\ing_software\\proyecto-1-TodoList\\TodoList-frontend\\src\\components\\TareasCompletadas.vue"],"sourcesContent":["<template>\r\n    <div class=\"container\" v-if=\"hasUserIdCookie\">\r\n      <div class=\"box\">\r\n        <h1>Lista de Tareas Completadas</h1>\r\n        <div class=\"task-list\">\r\n          <table class=\"table-with-border\">\r\n            <thead>\r\n              <tr>\r\n                <th class=\"column-border\">ID</th>\r\n                <th class=\"column-border\">Tarea</th>\r\n                <th class=\"column-border\">Fecha de Completado</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"completedTask in filteredCompletedTasks\" :key=\"completedTask.id\">\r\n                <td class=\"column-border\">{{ completedTask.id }}</td>\r\n                <td class=\"column-border\">{{ completedTask.task_name }}</td>\r\n                <td class=\"column-border\">{{ completedTask.completion_date }}</td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import Cookies from 'js-cookie';\r\n  import axios from 'axios';\r\n  \r\n  export default {\r\n    name: 'TareasCompletadas',\r\n    props: {\r\n      filteredTasks: Array, // List of filtered tasks from AppTareas\r\n    },\r\n    data() {\r\n      return {\r\n        completedTasks: [], // To store the fetched completed tasks\r\n      };\r\n    },\r\n    computed: {\r\n      hasUserIdCookie() {\r\n        return !!Cookies.get('userId'); // Check if the cookie 'userId' is present\r\n      },\r\n      // Filter completed tasks based on matching task IDs with filtered tasks\r\n      filteredCompletedTasks() {\r\n        const taskIds = this.filteredTasks.map(task => task.id);\r\n        return this.completedTasks.filter(completedTask => taskIds.includes(completedTask.task_id));\r\n      },\r\n    },\r\n    mounted() {\r\n      // Fetch completed tasks from the 'completed-tasks' API endpoint\r\n      axios.get('http://localhost:8080/api/v1/completed-tasks/all')\r\n        .then(response => {\r\n          this.completedTasks = response.data;\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching completed tasks:', error);\r\n        });\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  .box {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    flex-direction: column;\r\n    backdrop-filter: blur(30px);\r\n    background-color: rgba(248, 248, 248, 0.135);\r\n    border-radius: 5vh;\r\n    width: 95%;\r\n    padding-top: 1vh;\r\n    padding-bottom: 3vh;\r\n  }\r\n  .task-list {\r\n    color: white;\r\n  }\r\n  .column-border {\r\n    border: 1px solid white;\r\n  }\r\n  .container {\r\n    height: 59vh;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n    padding-bottom: 12vh;\r\n  }\r\n  </style>\r\n  "],"mappings":"AA2BE,OAAOA,OAAM,MAAO,WAAW;AAC/B,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,KAAK,EAAE;IACLC,aAAa,EAAEC,KAAK,CAAE;EACxB,CAAC;;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,EAAE,CAAE;IACtB,CAAC;EACH,CAAC;;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,OAAO,CAAC,CAACT,OAAO,CAACU,GAAG,CAAC,QAAQ,CAAC,EAAE;IAClC,CAAC;;IACD;IACAC,sBAAsBA,CAAA,EAAG;MACvB,MAAMC,OAAM,GAAI,IAAI,CAACR,aAAa,CAACS,GAAG,CAACC,IAAG,IAAKA,IAAI,CAACC,EAAE,CAAC;MACvD,OAAO,IAAI,CAACR,cAAc,CAACS,MAAM,CAACC,aAAY,IAAKL,OAAO,CAACM,QAAQ,CAACD,aAAa,CAACE,OAAO,CAAC,CAAC;IAC7F;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACAnB,KAAK,CAACS,GAAG,CAAC,kDAAkD,EACzDW,IAAI,CAACC,QAAO,IAAK;MAChB,IAAI,CAACf,cAAa,GAAIe,QAAQ,CAAChB,IAAI;IACrC,CAAC,EACAiB,KAAK,CAACC,KAAI,IAAK;MACdC,OAAO,CAACD,KAAK,CAAC,iCAAiC,EAAEA,KAAK,CAAC;IACzD,CAAC,CAAC;EACN;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}