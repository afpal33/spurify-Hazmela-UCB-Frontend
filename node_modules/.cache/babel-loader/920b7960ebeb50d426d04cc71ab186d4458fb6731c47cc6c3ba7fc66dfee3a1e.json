s: [\r\n        './assets/background.jpg',\r\n        'image2.jpg',\r\n        'image3.jpg'\r\n      ],\r\n      currentIndex: 0\r\n    };\r\n  },\r\n  computed: {\r\n    currentImage() {\r\n      return this.images[this.currentIndex];\r\n    }\r\n  },\r\n  methods: {\r\n    nextImage() {\r\n      this.currentIndex = (this.currentIndex + 1) % this.images.length;\r\n    },\r\n    prevImage() {\r\n      this.currentIndex = (this.currentIndex - 1 + this.images.length) % this.images.length;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.container {\r\n  height: 59vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  padding-bottom: 1.2vh;\r\n}\r\n\r\n.about-box {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  flex-direction: column;\r\n  backdrop-filter: blur(30px);\r\n  background-color: rgba(248, 248, 248, 0.135);\r\n  border-radius: 5vh;\r\n  width: 95%;\r\n  background-color: #e4e2e2;\r\n  color: #007f1300;\r\n}\r\n\r\n.text-body {\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n  align-items: center;\r\n  justify-content: center;\r\n  text-align: center;\r\n}\r\n\r\nh1 {\r\n  color: #e2dede;\r\n  font-size: 7vh;\r\n}\r\n\r\np {\r\n  color: #e2dede;\r\n  font-size: 30px;\r\n}\r\n\r\n.image-slider {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 400px;\r\n}\r\n\r\n.slider-container {\r\n  position: relative;\r\n}\r\n\r\n.slider-image {\r\n  max-width: 100%;\r\n  max-height: 100%;\r\n  transition: opacity 0.3s ease-in-out;\r\n}\r\n\r\n.slider-arrow {\r\n  position: absolute;\r\n  top: 50%;\r\n  transform: translateY(-50%);\r\n  width: 40px;\r\n  height: 40px;\r\n  border: none;\r\n  background-color: rgba(0, 0, 0, 0.3);\r\n  color: #fff;\r\n  font-size: 24px;\r\n  cursor: pointer;\r\n  opacity: 0.7;\r\n  transition: opacity 0.3s ease-in-out;\r\n}\r\n\r\n.slider-arrow:hover {\r\n  opacity: 1;\r\n}\r\n\r\n.left-arrow {\r\n  left: 10px;\r\n}\r\n\r\n.right-arrow {\r\n  right: 10px;\r\n}\r\n</style>\r\n"],"mappings":"AAmBA,eAAe;EACbA,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,CACN,yBAAyB,EACzB,YAAY,EACZ,YAAW,CACZ;MACDC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,YAAYA,CAAA,EAAG;MACb,OAAO,IAAI,CAACH,MAAM,CAAC,IAAI,CAACC,YAAY,CAAC;IACvC;EACF,CAAC;EACDG,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACV,IAAI,CAACJ,YAAW,GAAI,CAAC,IAAI,CAACA,YAAW,GAAI,CAAC,IAAI,IAAI,CAACD,MAAM,CAACM,MAAM;IAClE,CAAC;IACDC,SAASA,CAAA,EAAG;MACV,IAAI,CAACN,YAAW,GAAI,CAAC,IAAI,CAACA,YAAW,GAAI,IAAI,IAAI,CAACD,MAAM,CAACM,MAAM,IAAI,IAAI,CAACN,MAAM,CAACM,MAAM;IACvF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}